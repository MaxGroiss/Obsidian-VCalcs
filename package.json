{
  "name": "obsidian-calcblocks",
  "version": "0.1.0",
  "description": "Python calculations to LaTeX in Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run src"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "latex",
    "python",
    "calculations"
  ],
  "devDependencies": {
    "@codemirror/autocomplete": "^6.20.0",
    "@codemirror/commands": "^6.10.0",
    "@codemirror/lang-python": "^6.1.6",
    "@codemirror/language": "^6.11.3",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.38.8",
    "@types/node": "^20.19.25",
    "@vitest/ui": "^2.1.9",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.8",
    "happy-dom": "^20.0.11",
    "obsidian": "latest",
    "typescript": "^5.3.2",
    "vitest": "^2.1.9"
  }
}
