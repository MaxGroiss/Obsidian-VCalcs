    "@codemirror/lang-python": "^6.1.6",
    "@codemirror/language": "^6.11.3",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.38.8",
    "@types/node": "^20.10.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.8",
    "obsidian": "latest",
    "typescript": "^5.3.2"
  }
}
